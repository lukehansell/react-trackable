{
  "name": "react-trackable",
  "version": "1.0.0",
  "description": "Adds data attributes for tracking to child components.",
  "main": "lib/trackable.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "build:watch": "babel -w src --out-dir lib",
    "demo:build": "webpack -w --config='webpack.config.js'",
    "demo:serve": "serve demo",
    "prepublish": "npm run test && npm run build",
    "start": "parallelshell 'npm run demo:serve' 'npm run demo:build'",
    "test": "mocha",
    "test:watch": "mocha -w"
  },
  "author": "Luke Hansell <luke.hansell@holidayextras.com>",
  "license": "ISC",
  "devDependencies": {
    "assert": "^1.3.0",
    "babel-cli": "^6.1.18",
    "babel-core": "^6.3.2",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "component-playground": "^1.0.4",
    "jsdom": "^7.1.1",
    "mocha": "^2.3.4",
    "parallelshell": "^2.0.0",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^0.14.2",
    "react-dom": "^0.14.2",
    "serve": "^1.4.0",
    "testdom": "^2.0.0",
    "webpack": "^1.12.6"
  },
  "dependencies": {
    "lodash.assign": "^3.2.0",
    "lodash.kebabcase": "^3.0.1",
    "react": "^0.14.2"
  }
}
